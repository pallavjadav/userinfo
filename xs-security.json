{
    "xsappname": "userinfo",
    "tenant-mode": "dedicated",
    "scopes": [
        {
            "name": "uaa.user",
            "description": "UAA"
        }
    ],
    "role-templates": [
        {
            "name": "Token_Exchange",
            "description": "UAA Token Exchange",
            "scope-references": [
                "uaa.user"
            ]
        }
    ]
    ,
    "oauth2-configuration": {
        "token-validity": 3600,
        "redirect-uris": [
            "https://*-userinfo-app.cfapps.*.hana.ondemand.com/login/callback"
        ]
    }
}